<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.himart.restservicecors.dao.AuthDao">
	<select id="checkAuth" parameterType="AuthDto" resultType="boolean">
			select count(*) from
			SV901M
			where Q_CODE = #{qcode}
			AND ORG_ID = #{orgId}
	</select>
</mapper>